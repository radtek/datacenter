body, form, td, th, a, p
{
    margin: 0;
    font-weight: normal;
    font-family: "宋体" ,tahoma;
    font-size: 12px;
}
body
{
    color: #000;
    background-color: #fff;
}
td
{
    word-break: break-all;
}
th
{
    text-align: center;
}
img
{
    border: 0;
}
a:link, a:visited
{
    color: #000066;
    text-decoration: none;
}
a:hover
{
    color: #0099ff;
    text-decoration: underline;
}

