<?xml version="1.0" encoding="utf-8"?>
<access-policy>
  <!--此文件最好放在站点的根目录下，以正常发挥作用-->
	<cross-domain-access>
		<policy>
			<allow-from http-request-headers="*">
				<domain uri="*"/>
			</allow-from>
			<grant-to>
				<resource path="/" include-subpaths="true"/>
			</grant-to>
		</policy>
	</cross-domain-access>
</access-policy>

