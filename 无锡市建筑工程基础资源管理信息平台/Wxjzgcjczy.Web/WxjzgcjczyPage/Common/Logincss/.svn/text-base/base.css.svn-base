@charset "utf-8";
/*公共样式*/
html,body {margin:0; padding:0;height:100%;height:auto!important;width:100%;border:none;font-family:Arial, Helvetica, sans-serif; }
div,form,ul,ol,li,dl,dt,dd,cite,p,input{margin:0; padding:0;}
h1,h2,h3,h4,h5,h6 { margin:0; padding:0;}
img{ margin:0; padding:0; border:none;list-style-type:none;}
input{font-size:12px;color:#333;list-style:none; height:16px;}
ol,ul,li,textarea,select{ list-style-type:none;}
input{ background-color:#FFFFFF; border:0px; width:180px; text-indent:3px; height:24px; border:solid 1px #ccc;}
.clear{ clear:both;font-size:12px;}
.fl{float:left}
.fr{float:right}
a,a:visited{color:#333333; text-decoration:none;}
a:hover{color:#ff7800; text-decoration: underline;}
* html .clearfix { height: 1%; }
h1,h2,h3{font-size:14px;}
.hide{display:none;}
.line-24{ line-height:24px;}
.ac:link {
	text-decoration: none; font-size:14px; font-weight:bold;
	color: #ffffff;
}
.ac:visited {
	text-decoration: none;font-size:14px; font-weight:bold;
	color: #ffffff;
}
.ac:hover {
	text-decoration: underline;
	color: #ffff00;font-size:14px; font-weight:bold;
}
.ac:active {
	text-decoration: none;font-size:14px; font-weight:bold;
	color: #F16F01;
}
/*颜色链接*/
.white{color:#ffffff;}
.orange{color:#ff6600;}
.black a,.black a:visited{color:#000; text-decoration:none}
.black {color:#000; font-weight:bold;}
.blue{color:#2f4982; font-size:14px;font-weight:bold;}
.blue1{color:#2f4982; font-size:14px;}
.blue2{color:#2f4982; font-size:12px;}
.red{ color:#ff0000;font-weight:bold;}
.blue a:hover{color:#55aadd; text-decoration:underline}
.gray a,.gray a:visited{color:#787878; text-decoration:none}
.gray{color:#787878;font-size:16px;font-weight:bold;}
.textarea{ border:1px solid #BCBEBB;}
.nTab{
    float: left;
    width: 99%;
    margin: 0 auto;   
    margin-bottom:10px;
}
.nTab .TabTitle{
    clear: both;
    height: 30px;
	font-size:14px;
	font-weight:bold;
	float:left;
	overflow:hidden;
	padding-bottom:1px;
}
.nTab .TabTitle ul{
    border:0;
    margin:0;
    padding:0;
}
.nTab .TabTitle li{
    float: left;
    width: 120px;
    cursor: pointer;
    padding-top: 6px;
	
    padding-right: 0px;
    padding-left: 0px;
   height: 30px;
    list-style-type: none;
    margin:0 3px 0 0;
	text-align:center;
}
.nTab .TabTitle .active{background:#E8EDF3;color:#233A42;BORDER-left: #D9D9D9 1px solid;BORDER-right: #D9D9D9 1px solid;BORDER-top: #D9D9D9 1px solid;}
.nTab .TabTitle .normal{BORDER-left: #D9D9D9 1px solid;BORDER-right: #D9D9D9 1px solid;BORDER-top: #D9D9D9 1px solid;background:#fff;}
.nTab .TabContent{
    width:99%;
    margin: 0px auto;
	text-align:left;
    border:0px #D9D9D9 solid;
	float:left;
	font-size:14px;
	overflow:hidden;
}
.nTab .TabContent li{
    width:100%;
	text-align:center;
	margin-top:2px;
	margin-bottom:1px;
	padding-left:15px;
    height:206px;
	
	background: url(../LoginImage/tb1.jpg) no-repeat;
    float:left;
	overflow:hidden;
}
.none {display:none;}
.more { width:55px; height:25px; float:right; line-height:25px; padding-right:15px;}