//控制“高级搜索”的显示与隐藏
function trShow() {
    //招投标
    var ddl_ztb = document.getElementById("DDL_ztb");
    var ztbDate1 = document.getElementById("DB_ztbrq1");
    var ztbDate2 = document.getElementById("DB_ztbrq2");
    if (ddl_ztb.value == "True") {
        ztbDate1.disabled = false;
        ztbDate2.disabled = false;
        ztbDate1.style.backgroundColor = "#FFFFFF";
        ztbDate2.style.backgroundColor = "#FFFFFF";
    }
    else {
        ztbDate1.disabled = true;
