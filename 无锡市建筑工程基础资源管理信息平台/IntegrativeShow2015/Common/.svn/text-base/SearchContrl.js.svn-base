        jgbaDate1.style.backgroundColor = "#EEEEEE";
        jgbaDate2.style.backgroundColor = "#EEEEEE";
    }
    //高级查询
    if (document.all.tr_search.style.display == "none") {
        document.all.tr_search.style.display = "block";
        document.all.hd_status.value = "1";
    }
    else {
        document.all.tr_search.style.display = "none";
        document.all.hd_status.value = "0";
